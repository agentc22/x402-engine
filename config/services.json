    {
      "id": "image-fast",
      "name": "Fast Image Generation",
      "description": "Quick image generation using FLUX Schnell (~2s, good for drafts)",
      "price": "$0.015",
      "method": "POST",
      "path": "/api/image/fast",
      "mimeType": "application/json",
      "category": "compute",
      "upstream": {
        "provider": "fal",
        "baseUrl": "https://fal.run/fal-ai/flux/schnell"
      },
      "parameters": {
        "body": {
          "prompt": {
            "type": "string",
            "required": true,
            "description": "Text description of image to generate",
            "example": "a red sports car"
          },
          "image_size": {
            "type": "string",
            "required": false,
            "description": "Output size",
            "example": "square_hd",
            "default": "square_hd"
          },
          "num_images": {
            "type": "number",
            "required": false,
            "description": "Number of images (1-4)",
            "example": 1,
            "default": 1
          }
        }
      }
    },
    {
